view.container {
  margin: 0 20rpx;
}
view.container view.item {
  margin-top: 10rpx;
  background-color: #fff;
  padding: 20rpx;
  border-radius: 6rpx;
}
view.container view.item view.head {
  display: flex;
  justify-content: space-between;
  font-size: 24rpx;
  color: #666;
}
view.container view.item view.head view.countDown {
  display: flex;
}
view.container view.item view.head view.countDown .van-count-down {
  font-size: 22rpx;
  color: #666;
}
view.container view.item view.main view.desc {
  background-color: #f9f9f9;
  font-size: 24rpx;
  margin: 10rpx 0;
  padding: 10rpx;
  line-height: 30rpx;
  color: #4d4d4d;
}
view.container view.item view.main view.desc view.price_box {
  display: flex;
}
view.container view.item view.main view.desc view.price_box view.price {
  color: #ff3f34;
}
view.container view.item view.footer view.viewDetail {
  display: flex;
  justify-content: flex-end;
  padding-right: 30rpx;
}
view.container view.item view.footer view.viewDetail .van-button .van-button__text {
  font-size: 28rpx;
}
