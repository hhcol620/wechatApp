page {
  background-color: #f6f6f6;
}
.noreal {
  position: relative;
  height: 100vh;
}
.noreal view.uploadImg {
  margin-top: 20rpx;
  padding: 0 30rpx;
  height: 460rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
}
.noreal view.uploadImg .left.item {
  width: calc(100vw - 60rpx);
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.noreal view.uploadImg .left.item .upbox {
  width: calc(100vw - 60rpx);
  box-sizing: border-box;
  border: 2rpx dashed #999;
  height: 360rpx;
}
.noreal view.uploadImg .left.item .upbox .beforeUpload .icon {
  position: relative;
  height: 360rpx;
}
.noreal view.uploadImg .left.item .upbox .beforeUpload .icon .col {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 100rpx;
  height: 10rpx;
  background-color: #999;
}
.noreal view.uploadImg .left.item .upbox .beforeUpload .icon .row {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 10rpx;
  height: 100rpx;
  background-color: #999;
}
.noreal view.uploadImg .left.item .upbox image {
  width: calc(100vw - 60rpx);
  height: 100%;
}
.noreal view.uploadImg .left.item .title {
  margin-top: 10rpx;
  line-height: 40rpx;
  font-size: 26rxp;
  color: #4d4d4d;
  font-style: normal;
  font-weight: 500;
  text-align: center;
}
.noreal view.form {
  background-color: #fff;
  margin: 20rpx 0;
  padding: 0 20rpx;
}
.noreal view.form .van-cell::after {
  margin-right: 30rpx;
}
.noreal .submit {
  position: absolute;
  bottom: 100rpx;
  left: 30rpx;
}
.noreal .submit button {
  width: calc(100vw - 60rpx);
}
