<!-- 这是浏览历史的组件 首先顶部使用粘性布局 -->
<view class="sticky">
  <view class="box">
    <view class="title">我浏览的</view>
    <view class="deleteBtn" bind:tap="deleteAll">清空</view>
  </view>
</view>
<view class="container">
  <view class="item_b">
    <view class="time_title">这是时间</view>
    <view class="content_b">
      <view class="item" bind:longpress="longPressFunc">
        <view class="image left">
          <image class="" src="https://image.suning.cn/uimg/ZR/share_order/158742573131221711.jpg" mode="aspectFill" lazy-load="false" binderror="" bindload=""></image>
        </view>
        <view class="right">
          <view class="desc">这是描述</view>
          <view class="price_sign">
            <view class="price">￥ 这是价格</view>
            <view class="sign">商品</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>
<!-- 删除浏览记录的弹框  删除单个和删除全部公用一个 -->
<van-dialog id="van-dialog" />