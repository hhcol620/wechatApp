<!-- 这是浏览历史的组件 首先顶部使用粘性布局 -->
<view class="sticky">
  <view class="box">
    <view class="title">我浏览的</view>
    <view class="deleteBtn" bind:tap="deleteAll">清空</view>
  </view>
</view>
<view class="container" wx:for="{{recordList}}"  wx:key="id" wx:for-index="id" wx:for-item="it" >
  <view class="item_b" >
    <view class="time_title">{{it.browseDate}}</view>
    <view class="content_b">
      <view class="item" bind:longpress="longPressFunc">
        <view class="image">
          <image class="" src="" mode="aspectFill" lazy-load="false" binderror="" bindload=""></image>
        </view>
        <view class="price">{{it.salePrice}}</view>
      </view>
    </view>
  </view>
</view>

<!-- 删除浏览记录的弹框  删除单个和删除全部公用一个 -->
<van-dialog id="van-dialog" />
