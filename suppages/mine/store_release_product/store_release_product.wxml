<!-- 遮罩弹框提示用户  显示内容 选择更多  查看钱款 查看物流 -->
<van-overlay show="{{ isShow }}" bind:click="onClickHide">
  <view class="overlay_container" hover-class="none" hover-stop-propagation="false">
    <view class="block">
      <!-- 查看买家信息 -->
      <view class="delete_commodity item" bind:tap="delete_commodity">下架商品</view>
      <!-- 查看物流 -->
      <view class="review_logistics item" bind:tap="review_logistics">查看物流</view>
      <!-- 查看订单信息 -->
      <view class="order_detail item" bind:tap="order_detail">查看订单信息</view>
      <!-- 查看钱款去向 这个可以使用弹框告诉用户 -->
      <view class="where_monery item" bind:tap="where_monery">查看钱款去向</view>
    </view>
  </view>
</van-overlay>