<!-- 这是我的里面商城中的订单 这里面包括着我买到的和我卖出的订单 -->
<van-tabs active="{{ active }}" color='var(--themeColor)' bind:change="tabChange">
  <van-tab title="我买到的">
    <store_orders_buy bind:detail="toOrdersDetail" myOrderList="{{orderList}}" imgURL="{{imgURL}}"></store_orders_buy>
  </van-tab>
  <van-tab title="我卖出的">
    <store_orders_sale bind:detail="toOrdersDetail" myOrderList="{{orderList}}" imgURL="{{imgURL}}"></store_orders_sale>
  </van-tab>
</van-tabs>