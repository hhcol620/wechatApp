page {
  background-color: #f9f9f9;
}
.container {
  height: 100vh;
  width: 100vw;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.container image {
  margin-right: 60rpx;
  -webkit-filter: grayscale(80%);
  /* Chrome, Safari, Opera */
  filter: grayscale(80%);
}
.container .tip {
  margin: 20rpx;
  color: #666;
  font-size: 36rpx;
  line-height: 60rpx;
}
