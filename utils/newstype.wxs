// 这里写wxs代码  将消息格式化  给的数字 显示对应的文字
/* 
  10-商城留言  20-论坛留言  30-跑腿订单通知  40-商城订单通知 50-商城公益基金 60-商城评价  70-其他系统通知  80 商品审核 85 需求审核  90 文章审核
*/
var newsType = function (num) {
  // 根据不同的num值 返回不同的内容
  // let n = num
  var result = '其他留言';
  switch (num) {
    case 10:
      result = "商城留言"
      break;
      case 20:
        result = "论坛留言"
      break;
      case 30:
        result = "跑腿订单通知"
      break;
      case 40:
        result = "商城订单通知"
      break;
      case 50:
        result = "商城公益基金"
      break;
      case 60:
        result = "商城评价"
      break;
      case 70:
        result = "其他系统通知"
      break;
      case 80:
        result = "商品审核"
      break;
      case 85:
        result = "需求审核"
      break;
      case 90:
        result = "文章审核"
      break;
    default:
      result = "其他留言"
      break;
  }
  return result
}

module.exports = {
  newsType:newsType
};