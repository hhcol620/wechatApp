view view.selectInputContainer {
  padding: 10rpx;
  background-color: var(--themeColor);
}
view view.historyContainer {
  padding: 20rpx;
}
view view.historyContainer view.titleCon text {
  font-size: 30rpx;
  font-weight: 600;
  line-height: 45rpx;
  color: #4d4d4d;
}
view view.historyContainer view.tags {
  margin-top: 30rpx;
  display: flex;
  flex-wrap: wrap;
}
view view.historyContainer view.tags text.tag {
  font-size: 26rpx;
  line-height: 38rpx;
  padding: 6rpx 10rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  color: #666666;
  background-color: #eee;
  margin: 10rpx 6rpx;
}
