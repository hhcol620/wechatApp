view view.selectInputContainer {
  padding: 10rpx;
  background-color: var(--themeColor);
}
view view.historyContainer {
  margin: 20rpx;
  padding: 20rpx;
}
view view.historyContainer view.titleCon {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
view view.historyContainer view.titleCon view {
  font-size: 26rpx;
  font-weight: 500;
  line-height: 45rpx;
  color: #666;
}
view view.historyContainer view.titleCon view.iconfont {
  font-size: 40rpx;
  font-weight: 500;
  line-height: 45rpx;
  color: #666;
}
view view.historyContainer view.tags {
  margin-top: 20rpx;
  display: flex;
  flex-wrap: wrap;
}
view view.historyContainer view.tags view.tag {
  font-size: 26rpx;
  line-height: 38rpx;
  padding: 6rpx 10rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  color: #666666;
  background-color: #eee;
  margin: 10rpx 6rpx;
}
